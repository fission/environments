{
  "NugetSpecsFile": "nuget.txt",
  "DllExcludeFile": "exclude.txt",
  "BuildLogDirectory": "logs",
  "DllDirectory": "Dlls",
  "NugetPackageRegEx": "\\s*(?<package>[^:\\n]*)(?:\\:)?(?<version>.*)?",
  "ExcludeDllRegEx": "\\:?\\s*(?<package>[^:\\n]*)(?:\\:)?(?<dll>.*)?",
